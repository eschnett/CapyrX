  FormBox[
   RowBox[{"(", 
    RowBox[{"x", ",", "y", ",", "z"}], ")"}], TraditionalForm]],
  ExpressionUUID->"1bd5ddbc-7d0b-42c6-9ef9-e018ca45a581"],
